using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace ConsoleApp2
{
    internal class Program
    {
        static void Main(string[] args)
        {

            Console.WriteLine("Enter two numbers that one of them must be 9 , and we will solve it for u on another way");
            int num1 = int.Parse(Console.ReadLine());
            int num2 = int.Parse(Console.ReadLine());

            if (num2 > num1)
            {

                int res = num1 - 1;
                int sum = num2 - res;
                Console.Write(res);
                Console.WriteLine(sum);

            }

            else if (num1 > num2)
            {
                int res = num2 - 1;
                int sum = num1 - res;

                Console.Write(res);
                Console.Write(sum);
                Console.WriteLine();
            }
            Console.WriteLine("we solve it without multiplication , in this way:" +
                "First, we take the smaller number and subtract one from it, " +
                "then we calculate the difference between the new number and the larger number and put the result next to the new number " +
                "so that they appear adjacent as if they were the result of multiplying the two numbers that the user entered." +
                "This method is only for the multiplication table of the number 9.");


        }
    }
}
